                            android:id="@+id/ll_voucher_container"
                            android:visibility="gone"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="light"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="14sp"
                                android:layout_marginTop="@dimen/padding"
                                android:text="@string/voucher"
                                android:textColor="@color/ny_grey4" />

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="bold"
                                android:id="@+id/tv_voucher_total"
                                android:gravity="right|end"
                                android:layout_weight="1"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/padding"
                                android:layout_marginLeft="@dimen/padding"
                                android:layout_marginStart="@dimen/padding"
                                android:textSize="16sp"
                                android:text="Rp 0"
                                android:textColor="@color/ny_grey4" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_additional_container"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="reguler"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="14sp"
                                android:layout_marginTop="@dimen/padding"
                                android:text="@string/total_price"
