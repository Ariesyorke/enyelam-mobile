                                app:fontTextStyle="mediumItalic" />

                            <!--<com.nyelam.android.view.font.NYTextView-->
                            <!--android:id="@+id/change_contact_textView"-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="wrap_content"-->
                            <!--android:text="@string/change"-->
                            <!--android:textAllCaps="false"-->
                            <!--android:textColor="@color/ny_blue7"-->
                            <!--android:textSize="14sp"-->
                            <!--android:textStyle="bold" />-->
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/padding"
                            android:orientation="horizontal">

                            <com.nyelam.android.view.font.NYTextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/phone_number"
                                android:textColor="@color/ny_grey4"
                                android:textSize="14sp"
                                app:fontTextStyle="reguler" />

                            <com.nyelam.android.view.font.NYTextView
                                android:id="@+id/tv_billing_phone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/ny_black1"
                                android:textSize="14sp"
                                app:fontTextStyle="reguler"
                                tools:text="081727727" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/padding"
                            android:orientation="horizontal">

                            <com.nyelam.android.view.font.NYTextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/email"
                                android:textColor="@color/ny_grey4"
                                android:textSize="14sp"
                                app:fontTextStyle="reguler" />

                            <com.nyelam.android.view.font.NYTextView
                                android:id="@+id/tv_billing_email"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/ny_black1"
                                android:textSize="14sp"
                                app:fontTextStyle="reguler"
                                tools:text="example@mail.com" />
                        </LinearLayout>


                        <LinearLayout
