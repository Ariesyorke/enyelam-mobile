                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <com.nyelam.android.view.font.NYTextView
                            app:fontTextStyle="mediumItalic"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/ny_blue7"
                            android:paddingLeft="@dimen/activity_horizontal_margin"
                            android:paddingTop="@dimen/padding"
                            android:paddingBottom="@dimen/padding"
                            android:text="@string/payment"
                            android:textColor="@color/colorWhite"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:orientation="vertical"
                            android:background="@color/colorWhite"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="mediumItalic"
                                android:text="@string/transfer_to"
                                android:textSize="18sp"
                                android:textColor="@color/ny_black1"
                                android:gravity="center"
                                android:padding="@dimen/padding_half"
                                android:layout_marginTop="@dimen/activity_horizontal_margin"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" />

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="mediumItalic"
                                android:layout_marginBottom="@dimen/padding"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:layout_gravity="center"
                                android:textSize="18sp"
                                android:visibility="gone"
                                android:text="@string/bank_branch"
                                android:textAppearance="?android:attr/textAppearanceMedium" />

                            <TableLayout
                                android:id="@+id/tabla_cuerpo"
                                android:paddingLeft="@dimen/padding_double"
                                android:paddingRight="@dimen/padding_double"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <TableRow
                                    android:id="@+id/tableRow4"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <com.nyelam.android.view.font.NYTextView
                                        app:fontTextStyle="reguler"
                                        android:id="@+id/textView1"
                                        android:layout_weight="2"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:padding="5dp"
                                        android:gravity="left"
                                        android:textSize="14sp"
                                        android:text="@string/account_number_label"
                                        android:textAppearance="?android:attr/textAppearanceMedium" />

                                    <com.nyelam.android.view.font.NYTextView
                                        app:fontTextStyle="reguler"
                                        android:id="@+id/textView2"
                                        android:layout_weight="3"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:padding="5dp"
                                        android:gravity="center"
                                        android:textSize="14sp"
                                        android:text="@string/account_number"
                                        android:textAppearance="?android:attr/textAppearanceMedium" />

                                </TableRow>

                                <TableRow
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <com.nyelam.android.view.font.NYTextView
                                        app:fontTextStyle="reguler"
                                        android:layout_weight="2"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:padding="5dp"
                                        android:gravity="left"
                                        android:textSize="14sp"
                                        android:text="@string/account_name_label"
