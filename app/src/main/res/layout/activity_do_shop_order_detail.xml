                            <!--android:textColor="@color/ny_blue7"-->
                            <!--android:textSize="14sp"-->
                            <!--android:textStyle="bold" />-->
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/padding"
                            android:orientation="horizontal">

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="reguler"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/phone_number"
                                android:textColor="@color/ny_grey4"
                                android:textSize="14sp" />

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="reguler"
                                android:id="@+id/tv_shipping_phone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                tools:text="081727727"
                                android:textColor="@color/ny_black1"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/padding"
                            android:orientation="horizontal">

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="reguler"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/email"
                                android:textColor="@color/ny_grey4"
                                android:textSize="14sp" />

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="reguler"
                                android:id="@+id/tv_shipping_email"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                tools:text="example@mail.com"
                                android:textColor="@color/ny_black1"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/padding"
                            android:orientation="horizontal">

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="reguler"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/address"
                                android:textColor="@color/ny_grey4"
                                android:textSize="14sp" />

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="reguler"
                                android:id="@+id/tv_shipping_address"
                                android:layout_weight="1"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                tools:text="Jalan Pjajaran Bogor"
                                android:textColor="@color/ny_black1"
                                android:textSize="14sp" />
                        </LinearLayout>


                    </LinearLayout>


                    <com.nyelam.android.view.font.NYTextView
                        app:fontTextStyle="medium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/ny_blue7"
                        android:paddingLeft="@dimen/activity_horizontal_margin"
                        android:paddingTop="@dimen/padding"
                        android:paddingBottom="@dimen/padding"
                        android:text="@string/total_summary"
                        android:textColor="@color/colorWhite"
                        android:textStyle="bold" />


                    <LinearLayout
                        android:padding="@dimen/activity_horizontal_margin"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="light"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/padding"
                                android:text="@string/sub_total"
                                android:textSize="14sp"
                                android:textColor="@color/ny_grey4" />

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="bold"
                                android:id="@+id/tv_sub_total"
                                android:gravity="right|end"
                                android:layout_weight="1"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/padding"
                                android:layout_marginLeft="@dimen/padding"
                                android:layout_marginStart="@dimen/padding"
                                android:textSize="16sp"
                                android:text="Rp 0"
                                android:textColor="@color/ny_grey4" />
                        </LinearLayout>


                        <LinearLayout
                            android:visibility="gone"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="light"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/padding"
                                android:text="@string/shipping_cost"
                                android:textSize="14sp"
                                android:textColor="@color/ny_grey4" />

                            <com.nyelam.android.view.font.NYTextView
                                app:fontTextStyle="bold"
                                android:id="@+id/tv_shipping_cost"
                                android:gravity="right|end"
                                android:layout_weight="1"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/padding"
                                android:layout_marginLeft="@dimen/padding"
                                android:layout_marginStart="@dimen/padding"
                                android:textSize="16sp"
                                android:text="Rp 0"
                                android:textColor="@color/ny_grey4" />
                        </LinearLayout>


